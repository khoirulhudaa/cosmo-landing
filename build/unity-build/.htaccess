<IfModule mod_mime.c>
  AddType application/wasm .wasm
  AddType application/javascript .js
  AddType application/octet-stream .data
</IfModule>

<IfModule mod_headers.c>
  Header set Access-Control-Allow-Origin "*"
  Header set Cross-Origin-Embedder-Policy "require-corp"
  Header set Cross-Origin-Opener-Policy "same-origin"
</IfModule>

# File WebAssembly
<Files "73b6b973f99e58b13f9e27103060aa86.wasm">
  AddType application/wasm .wasm
</Files>

# File data Unity
<Files "c06bda4de5b34d43cc5f1f0a5fa261fc.data">
  AddType application/octet-stream .data
</Files>

# File JavaScript loader Unity
<Files "f52347dc2ab1959d1e759a6f582b4b30.js">
  AddType application/javascript .js
</Files>

# Untuk file hasil kompresi Brotli (kalau Unity kamu aktifkan compression)
<FilesMatch "\.br$">
  AddEncoding br
</FilesMatch>
